{
  "manifest_version": 3,
  "name": "Convox â€“ Person B Test Harness",
  "version": "0.1.3",
  "description": "Quick-test the NLU + actions without other parts.",
  "permissions": ["activeTab", "storage"],
  "host_permissions": [
    "https://*.instructure.com/*",
    "http://localhost:11434/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.instructure.com/*"],
      "js": ["content_test.js"],
      "run_at": "document_idle",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["lib/*.js"],
      "matches": ["https://*.instructure.com/*"]
    }
  ]
}
